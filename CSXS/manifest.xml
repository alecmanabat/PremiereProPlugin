<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="4.0" ExtensionBundleId="com.adobe.ExportFileNames" ExtensionBundleVersion="11.1"
ExtensionBundleName="Export File Names" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.adobe.ExportFileNames" Version="1.0.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PPRO" Version="9" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="6.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.adobe.ExportFileNames">
			<DispatchInfo >
				<Resources>
					<MainPath>./index.html</MainPath>
					<ScriptPath>./ExportFileNames.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<CEFCommandLine>
					<Parameter>--allow-file-access</Parameter>
					<Parameter>--allow-file-access-from-files</Parameter>
					<Parameter>--enable-nodejs</Parameter>
				</CEFCommandLine>
				<UI>
					<Type>Panel</Type>
					<Menu>Export File Names</Menu>
					<Geometry>
						<Size>
							<Height>600</Height>
							<Width>700</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
